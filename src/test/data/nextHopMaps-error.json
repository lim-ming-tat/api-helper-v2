            },
            "case9": {
                "expected": "Source:\n{\n    \"propertyName\": \"field1\",\n    \"equal\": {\n        \"propertyName\": \"invalidName\"\n    }\n}\n\nError Message:\n.propertyName\n  .equal.propertyName is missing/undefined"
            },
            "case10": {
                "expected": "Source:\n{\n    \"propertyName\": \"field1\",\n    \"equal\": {}\n}\n\nError Message:\n.propertyName|dataValue\n  .equal.propertyName and .equal.dataValue is missing/undefined"
            },
            "case11": {
                "expected": "Source:\n{\n    \"notEqual\": \"some data\"\n}\n\nError Message:\n.propertyName\n  .propertyName is missing/undefined"
            },
            "case12": {
                "expected": "Source:\n{\n    \"propertyName\": \"invalidName\",\n    \"notEqual\": \"some data\"\n}\n\nError Message:\n.propertyName\n  .propertyName is missing/undefined"
            },
            "case13": {
                "expected": "Source:\n{\n    \"propertyName\": \"field1\",\n    \"notEqual\": {\n        \"propertyName\": \"invalidName\"\n    }\n}\n\nError Message:\n.propertyName\n  .notEqual.propertyName is missing/undefined"
            },
            "case14": {
                "expected": "Source:\n{\n    \"propertyName\": \"field1\",\n    \"notEqual\": {}\n}\n\nError Message:\n.propertyName|dataValue\n  .notEqual.propertyName and .notEqual.dataValue is missing/undefined"
            },
            "case15": {
                "expected": "Source:\n{\n    \"greaterThan\": \"some data\"\n}\n\nError Message:\n.propertyName\n  .propertyName is missing/undefined"
            },
            "case16": {
                "expected": "Source:\n{\n    \"propertyName\": \"invalidName\",\n    \"greaterThan\": \"some data\"\n}\n\nError Message:\n.propertyName\n  .propertyName is missing/undefined"
            },
            "case17": {
                "expected": "Source:\n{\n    \"propertyName\": \"field1\",\n    \"greaterThan\": {\n        \"propertyName\": \"invalidName\"\n    }\n}\n\nError Message:\n.propertyName\n  .greaterThan.propertyName is missing/undefined"
            },
            "case18": {
                "expected": "Source:\n{\n    \"propertyName\": \"field1\",\n    \"greaterThan\": {}\n}\n\nError Message:\n.propertyName|dataValue\n  .greaterThan.propertyName and .greaterThan.dataValue is missing/undefined"
            },
            "case19": {
                "expected": "Source:\n{\n    \"conditions\": true,\n    \"filters\": []\n}\n\nError Message:\n.conditions\n  .conditions must be a string"
            },
            "case20": {
                "expected": "Source:\n{\n    \"conditions\": \"invalidCondition\",\n    \"filters\": []\n}\n\nError Message:\n.conditions\n  .conditions must be (and|or)"
            },
            "case21": {
                "expected": "Source:\n{\n    \"conditions\": \"or\",\n    \"filters\": \"\"\n}\n\nError Message:\n.filters\n  .filters must be an array"
