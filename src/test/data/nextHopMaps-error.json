            },
            "case5": {
                "expected": "Source:\n{\n    \"propertyName\": \"field3\",\n    \"hasProperty\": \"true\"\n}\n\nError Message:\n.hasProperty\n  .hasProperty must be a boolean"
            },
            "case6": {
                "expected": "Source:\n{\n    \"hasProperty\": false\n}\n\nError Message:\n.propertyName\n  .propertyName is missing/undefined"
            },
            "case7": {
